		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.20",
		"electron": "^31.0.2",
		"electron-builder": "^24.13.3",
		"electron-vite": "^3.1.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.34.3",
		"postcss": "^8.4.47",
